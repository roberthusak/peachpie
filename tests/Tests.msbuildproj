  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="../src/Peachpie.App/Peachpie.App.csproj" />
	<ProjectReference Include="../src/PDO/Peachpie.Library.PDO.Sqlite/Peachpie.Library.PDO.Sqlite.csproj" />
  </ItemGroup>

</Project>